<%= simple_form_for(@bootcamp) do |f| %>
  <%= f.input :title %>
  <%= f.input :date %>
  <%= f.input :start_at%>
  <%= f.input :end_at%>
  <%= f.input :description%>
  <%= f.input :price%>
  <%= f.input :max_participation%>
  <%= f.input :coach, collection: Bootcamp::COACH%>
  <%= f.input :address, collection: Bootcamp::ADDRESS%>
  <%= f.input :title_address, collection: Bootcamp::TITLE_ADDRESS%>
  <%= f.submit "#{ bootcamp.persisted? ? 'Update' : 'Create' } cours" %>
  <%#= link_to 'Back to restaurant', restaurants_path %>
<% end %>
